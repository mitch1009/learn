generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           String   @id @default(cuid())
  firstName    String
  lastName     String
  email        String   @unique
  phone        String?  @unique
  age          Int?
  username     String?  @unique
  locationLang Float 
  locationLat  Float 
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
